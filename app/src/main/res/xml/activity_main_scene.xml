<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        app:constraintSetEnd="@layout/activity_main_motion_end"
        app:constraintSetStart="@layout/activity_main_motion_start"
        app:duration="1000"
        app:motionInterpolator="cubic(0,2,0,2,0,43,0,67)">
        <OnSwipe
            app:dragDirection="dragUp"
            app:touchAnchorSide="top" />
        <OnSwipe app:touchAnchorId="@+id/nested" />
    </Transition>
</MotionScene>
